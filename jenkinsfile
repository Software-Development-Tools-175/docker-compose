pipeline {
    agent any

    stages {

        stage('Welcome to main branch'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Build'){
            steps{
                script{
                    try{
                        echo ""
                    }catch(Exception e){
                        echo "Error occured"
                    }
                }
            }
        }

        stage('Trigger') {
            steps {
                echo 'Start Trigger to send member to slave'
                echo 'Triggering ...'
            }
        }
    }
}
